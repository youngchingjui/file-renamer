{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es5",
        "module": "es6",
        "strict": true,
        "jsx": "react",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "baseUrl": "src",
        "allowJs": true,
        "noImplicitAny": false,
        "paths": {
            "@/*": [
                "./*"
            ],
        },
    },
    "include": [
        "src/**/*"
    ]
}